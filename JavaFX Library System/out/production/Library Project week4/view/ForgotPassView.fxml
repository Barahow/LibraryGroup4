<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #2F4F4F;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ControllerForgotPass">
    <children>
        <Button layoutX="320.0" layoutY="100.0" mnemonicParsing="false" onAction="#sendPass" style="-fx-background-color: #039BE5;" text="OK">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
        <TextField fx:id="emailField" layoutX="150.0" layoutY="100.0" />
        <Label layoutX="50.0" layoutY="100.0" text="Enter email" textFill="#b8d92b">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
    </children>

</AnchorPane>
