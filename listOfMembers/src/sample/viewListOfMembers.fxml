<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="538.0" prefWidth="794.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.viewListOfMembers">
    <children>
        <VBox layoutX="5.0" prefHeight="407.0" prefWidth="785.0">
            <children>
                <TableView prefHeight="412.0" prefWidth="785.0">
                    <columns>
                        <TableColumn fx:id="memberidColumn" prefWidth="156.0" text="Member Library ID" />
                        <TableColumn fx:id="memberssnColumn" minWidth="0.0" prefWidth="154.0" text="Member SSN" />
                        <TableColumn fx:id="firstNameColumn" minWidth="0.0" prefWidth="122.0" text="first Name" />
                        <TableColumn fx:id="lastNameColumn" minWidth="0.0" prefWidth="155.0" text="Last Name" />
                    </columns>
                </TableView>
            </children>
        </VBox>
      <TextField fx:id="firstNameTextField" layoutX="227.0" layoutY="473.0" prefHeight="31.0" prefWidth="158.0" promptText="First Name" />
      <TextField fx:id="lastNameTextField" layoutX="24.0" layoutY="473.0" prefHeight="31.0" prefWidth="158.0" promptText="Last name" />
      <TextField fx:id="memberssnTextField" layoutX="24.0" layoutY="427.0" prefHeight="31.0" prefWidth="158.0" promptText="Member SSN" />
      <Button layoutX="420.0" layoutY="427.0" mnemonicParsing="false" onAction="#newMemberButtonPushed" prefHeight="31.0" prefWidth="316.0" text="Add new Member" />
      <TextField fx:id="memberidTextField" layoutX="227.0" layoutY="427.0" prefHeight="31.0" prefWidth="158.0" promptText="Member ID" />
      <Button layoutX="420.0" layoutY="473.0" mnemonicParsing="false" onAction="#deletedButtonPushed" prefHeight="31.0" prefWidth="316.0" text="Delete Member" />
    </children>
</AnchorPane>
